{
  "name": "spirebuff.app",
  "version": "0.1.0",
  "private": true,
  "main": "electron-starter.js",
  "homepage": "./",
  "scripts": {
    "test": "react-app-rewired test",
    "watch": "node watch.js",
    "electron:dev": "electron .",
    "dev": "npm-run-all --parallel watch electron:dev",
    "build": "react-app-rewired build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prod:pack": "npm-run-all build pack",
    "prod:dist": "npm-run-all build dist",
    "eject": "react-scripts eject",
    "make_card_json": "node -r dotenv/config scripts/make_card_json.js",
    "make_relic_json": "node -r dotenv/config scripts/make_relic_json.js",
    "make_event_json": "node -r dotenv/config scripts/make_event_json.js"
  },
  "build": {
    "extends": null,
    "appId": "spirebuff.app",
    "productName": "spirebuff",
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "files": [
      "./build/**/*",
      "electron-starter.js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "os": [
    "win32"
  ],
  "arch": [
    "x64"
  ],
  "dependencies": {
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.17",
    "@types/lodash": "^4.14.137",
    "@types/node": "12.7.1",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "cross-env": "^5.2.0",
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "redux": "^4.0.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.3",
    "typescript": "3.5.3",
    "use-react-router": "^1.0.7"
  }
}
